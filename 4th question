from ctypes.wintypes import MSG
import time
from selenium import webdriver
from selenium.webdriver.common.keys import Keys

driver = webdriver.Chrome(executable_path = "C:\\Users\\Ansar Jaha\\Desktop\M.Tech\\INTERNSHIP\\chromedriver.exe")
MSG = driver.find_element_by_class_name("alert.alert-danger")

driver.get("http://http://automationpractice.com/index.php")

time.sleep(5)

driver.find_element_by_class_name("login").click()
time.sleep(5)

driver.find_element_by_id("email_create").send_keys("mdapsarjaha@gmail.com")
time.sleep(5)

driver.find_element_by_xpath("//*[@id='SubmitCreate']/span").click()

driver.find_element_by_id("customer_firstname").send_keys("1234")

driver.find_element_by_id("customer_lastname").send_keys("56789")

driver.find_element_by_id('firstname').send_keys("1234")

driver.find_element_by_id('lastname').send_keys("5678")

driver.find_element_by_id('city').send_keys("7894")

driver.find_element_by_id('postcode').send_keys("abcd")

driver.find_element_by_id('phone').send_keys("abcdef")

driver.find_element_by_id('phone_mobile').send_keys("abcedfgh")

driver.find_element_by_id('submitAccount').click()



